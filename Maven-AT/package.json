        "serve": "allure serve target/allure-results",
        "init": "mvn clean install",
        "pkg": "mvn package",
        "deploy": "node index.js -deploy -repo -test",
        "clean": "rmdir /s /q repo\\org\\project\\Maven-AT\\1.0-SNAPSHOT",
        "deploy-manual": "npm run clean & mvn deploy:deploy-file -Durl=file:C:/Dev/OSPanel/domains/Java-AT/Maven-AT/repo -Dfile=target/Maven-AT-1.0-SNAPSHOT.jar -DgroupId=org.project -DartifactId=Maven-AT -Dpackaging=jar -Dversion=1.0-SNAPSHOT"
    },
    "devDependencies": {
        "xml2js": "^0.6.2"
    }
