{
    "name": "maven-at",
    "version": "7.8.4",
    "main": "index.js",
    "type": "module",
    "exports": {
        "default": "./index.js",
        "import": "./index.js",
        "require": "./index.js"
    },
    "license": "ISC",
    "author": {
        "name": "Nikolay Mishin",
        "url": "https://github.com/Nikolay-Mishin"
    },
    "description": "Library of development Java automatization",
    "keywords": [
        "java",
        "auto-qa"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Nikolay-Mishin/Java-AT",
