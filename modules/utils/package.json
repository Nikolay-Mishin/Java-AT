	"scripts": {
        "start": "npm run build & npm run deploy",
        "build": "mvn clean install",
        "pkg": "mvn clean package",
        "deploy": "npm run deploy:doc",
        "deploy:internal": "mvn clean deploy",
        "deploy:site": "mvn clean deploy -Dsite",
        "deploy:doc": "mvn clean deploy -Dsite -Ddoc",
        "deploy:src": "mvn clean deploy -Dsrc",
        "deploy:doc_src": "mvn clean deploy -Ddoc -Dsrc",
        "deploy:docker": "mvn clean deploy -Ddocker",
        "deploy:dockerfile": "mvn clean deploy -Ddockerfile",
        "profiles": "mvn help:active-profiles",
        "profiles:release": "mvn help:active-profiles -Drelease",
        "profiles:release-p": "mvn help:active-profiles -P release",
        "deploy-auto": "node index.js -deploy -root",
        "deploy-manual": "mvn clean deploy:deploy-file -Durl=file:C:/Dev/OSPanel/domains/Java-AT/modules/utils/repo -Dfile=target/utils-3.4.4-SNAPSHOT.jar -DgroupId=org.project -DartifactId=utils -Dpackaging=jar -Dversion=3.4.4-SNAPSHOT -Dsources=target/utils-3.4.4-SNAPSHOT-sources.jar -Djavadoc=target/Jutils-3.4.4-SNAPSHOT-javadoc.jar"
    }
